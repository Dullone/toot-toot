<div class="row">
  <div class="new-toot">
    <div class="field new-toot" id="new-toot-colapse">
      <div>Whats goin' on?</div>
    </div>
    <div id="toot-form">
    <%= form_for Toot.new, url: user_toots_path(current_user) do |f| %>
      <div class="field no-margin">
        <%= f.text_area :message %>
      </div>
      <div>
        <%= f.submit "Toot" %>
      </div>
    <% end %>
    </div>
  </div>
</div>